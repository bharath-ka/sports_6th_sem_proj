        <form action="/admin/adminEdit" method="post">

            <fieldset class="form-group">
                <legend class="lead">Your Info</legend>

                <div class="form-group">
                    <label class="form-control-label" for="name">First Name</label>
                    <input class="form-control" type="text" id="name" name="firstName"
                        value="<%= admins.firstName %>">
                </div><!-- form-group -->

                <div class="form-group">
                    <label class="form-control-label" for="name">Last Name</label>
                    <input type="text" class="form-control" id="name" name="lastName"
                     value="<%= admins.lastName %>">
                </div><!-- form-group -->

                <div class="form-group">
                    <label class="form-control-label" for="name">Username</label>
                    <input type="text" class="form-control" id="name" name="username"  value="<%= admins.username %>">
                </div>
                <div class="form-group">
                    <label class="form-control-label" for="name">Email</label>
                    <input type="text" class="form-control" id="name" name="email" 
                        value="<%= admins.email %>">
                </div>
                <div class="form-group">
                    <label class="form-control-label" for="name">Password</label>
                    <input type="text" class="form-control" id="name" name="password" 
                        value="<%= admins.password %>">
                </div>
                <div id="operations">
                    <button class="btn btn-primary" type="submit" value="Submit">Submit</button>
                </div>
            </fieldset><!-- fieldset -->
        </form>
